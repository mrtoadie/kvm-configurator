<?xml version="1.0" encoding="UTF-8"?>
<domain xmlns:qemu="http://libvirt.org/schemas/domain/qemu/1.0" type="kvm">
  <name>test</name>
  <memory unit="KiB">253952</memory>
  <currentMemory unit="KiB">253952</currentMemory>
  <vcpu placement="static">2</vcpu>
  <os>
    <type arch="x86_64" machine="pc-q35-2.9">hvm</type>
    <boot dev="hd"></boot>
  </os>
  <features>
    <acpi></acpi>
    <apic></apic>
    <pae></pae>
  </features>
  <clock offset="utc"></clock>
  <on_poweroff>destroy</on_poweroff>
  <on_reboot>restart</on_reboot>
  <on_crash>destroy</on_crash>
  <devices>
    <graphics type="vnc" port="-1" autoport="yes" listen="0.0.0.0"></graphics>
    <input type="mouse" bus="ps2"></input>
    <input type="keyboard" bus="ps2"></input>
    <console type="pty">
      <target type="serial" port="0"></target>
    </console>
  </devices>
</domain>